<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="train1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:train1"
    xmlns:pages="clr-namespace:train1.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="train1">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent         
        Route="MagicInformationPage" 
        ContentTemplate="{DataTemplate pages:MagicInformationPage}"/>

</Shell>
